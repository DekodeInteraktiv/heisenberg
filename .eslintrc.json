{
  "env": {
    "jest": true,
    "node": true
  },
  "extends": [
    "plugin:wordpress/recommended",
    "plugin:wordpress/node",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "newline-per-chained-call": "off",
    "no-console": "off",
    "no-process-exit": "off",
    "no-sync": "off",
    "node/no-missing-require": "off",
    "wordpress/array-function-spacing": "off"
  }
}
